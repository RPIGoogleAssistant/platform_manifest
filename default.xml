<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />
           
  <remote  name="rpigoogleassistant"
	   fetch="https://github.com/RPIGoogleAssistant" />          
     
  <default revision="master"
      remote="rpigoogleassistant"
      sync-j="4" />
  
  <project path="platform_manifest" name="platform_manifest" remote="rpigoogleassistant"/>
  <project path="gassistant" name="rpigassistant_main_module" remote="rpigoogleassistant"/>
  <project path="mpvplayer" name="rpigassistant_mpvplayer_module" remote="rpigoogleassistant"/>
  <project path="gmusicplayer" name="rpigassistant_gmusicplayer_module" remote="rpigoogleassistant"/>
  
</manifest>
